{
  "extends": "airbnb-base",
  "root": true,
  "rules": {
    "camelcase": [0],
    "max-len": [2, 100],
    "new-cap": [0],
    "no-case-declarations": [2],
    "no-confusing-arrow": [0],
    "no-else-return": [0],
    "no-multi-spaces": [0],
    "no-param-reassign": [0],
    "no-restricted-syntax": [
      2,
      "DebuggerStatement",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-shadow": [0],
    "no-use-before-define": [0],
    "prefer-template": [0],
    "quote-props": [0],
    "space-before-function-paren": [2, "never"],
    "strict": [2, "global"]
  }
}
